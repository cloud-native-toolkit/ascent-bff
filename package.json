{
  "name": "ascent-bff",
  "version": "0.0.1",
  "description": "Backend for Frontend of the Ascent tool.",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "compile": "npm run clean && lb-tsc",
    "compile:watch": "lb-tsc --watch",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "e2e": "echo 'We need to implement future E2E tests'",
    "package": "./scripts/packageApp.sh",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "NODE_ENV=test lb-mocha --allow-console-logs \"dist/__tests__\"",
    "unit-test": "NODE_ENV=test lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t ascent-bff .",
    "docker:run": "docker run -p 3001:3001 -d ascent-bff",
    "premigrate": "npm run compile",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run compile",
    "openapi-spec": "node ./dist/openapi-spec",
    "start": "NODE_ENV=staging node .",
    "start:dev": "NODE_ENV=dev node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run compile",
    "serve": "NODE_ENV=staging node .",
    "serve:dev": "NODE_ENV=dev node .",
    "serve:test": "NODE_ENV=test node .",
    "scan": "detect-secrets scan --update .secrets.baseline --exclude-files 'package-lock.json'"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "No√© Samaille <noe.samaille@ibm.com>",
  "license": "Apache-2.0",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@cloudnativetoolkit/iascable": "^3.2.2",
    "@loopback/boot": "^5.0.4",
    "@loopback/core": "^4.0.4",
    "@loopback/repository": "^5.0.4",
    "@loopback/rest": "^12.0.4",
    "@loopback/rest-explorer": "^5.0.4",
    "@loopback/service-proxy": "^5.0.4",
    "adm-zip": "^0.5.9",
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "csv-parse": "^5.3.2",
    "dotenv": "^16.0.3",
    "handy-redis": "^2.2.1",
    "ibm-cos-sdk": "^1.12.2",
    "ibmcloud-appid": "^6.2.6",
    "jimp": "^0.16.2",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.21",
    "loopback-connector-mongodb": "^5.6.0",
    "multer": "^1.4.5-lts.1",
    "nconf": "^0.12.0",
    "optional-js": "^2.3.0",
    "passport": "^0.4.1",
    "pdfjs": "^2.4.7",
    "redis": "^3.1.0",
    "tslib": "^2.0.0",
    "typescript-ioc": "^3.2.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.4",
    "@loopback/eslint-config": "^13.0.4",
    "@loopback/testlab": "^7.0.10",
    "@types/adm-zip": "^0.5.0",
    "@types/js-yaml": "^3.12.4",
    "@types/lodash": "^4.14.188",
    "@types/multer": "^1.4.7",
    "@types/nconf": "^0.10.3",
    "@types/node": "^14.18.32",
    "@types/redis": "^4.0.11",
    "eslint": "^8.25.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.8.4"
  }
}
